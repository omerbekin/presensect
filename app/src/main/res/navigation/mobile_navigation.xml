<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation.xml"
    app:startDestination="@id/navigation_feed">

    <fragment
        android:id="@+id/navigation_feed"
        android:name="com.petophion.presensect.activities.main.bottomnavbar.feed.FeedFragment"
        android:label="Feed"
        tools:layout="@layout/fragment_feed" />
    <fragment
        android:id="@+id/navigation_new_post"
        android:name="com.petophion.presensect.activities.main.bottomnavbar.newpost.NewPostFragment"
        android:label="New Post"
        tools:layout="@layout/fragment_new_post" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.petophion.presensect.activities.main.bottomnavbar.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" />
